{
  "name": "nestjs-discord-transporter",
  "version": "0.1.2",
  "description": "Discord event listener for Nestjs",
  "keywords": [
    "discord",
    "nestjs",
    "nestjs-transporter"
  ],
  "author": "Francisco Dami√£o Terto <contato@damiaoterto.dev>",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist/ @types/",
    "build": "babel src/ -d dist/ --extensions .ts,.js",
    "prepare": "husky install",
    "gen:types": "tsc -p tsconfig.build.json --declaration --allowJs --emitDeclarationOnly --outDir @types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/testing": "^9.4.0",
    "@types/node": "^18.15.11",
    "@vitest/coverage-istanbul": "^0.30.1",
    "babel-loader": "^9.1.2",
    "husky": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  }
}
